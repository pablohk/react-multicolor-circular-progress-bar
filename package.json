{
  "name": "react-multicolor-circular-progress-bar",
  "version": "1.0.0",
  "description": "Customizable multicolor circular and semicircular SVG progress bar",
  "author": "Pablo Carmona &lt;pablusmid@gmail.com&gt;",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pablohk/react-multicolor-circular-progress-bar"
  },
  "homepage": "https://github.io/react-multicolor-circular-progress-bar",
  "bugs": {
    "url": "https://github.com/pablohk/react-multicolor-circular-progress-bar/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepublish": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "test:coverage": "react-scripts test --coverage  --watchAll=false",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.0.0",
    "react-multicolor-circular-progress-bar": "latest"
  },
  "devDependencies": {
    "microbundle-crl": "^0.13.8",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-multicolor-circular-progress-bar": "latest",
    "prop-types": "^15.7.2",
    "react-scripts": "^3.4.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2"
  },
  "files": [
    "dist"
  ]
}
